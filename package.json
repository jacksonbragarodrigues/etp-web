{
  "name": "etp-web",
  "version": "3.6.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check",
    "local": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check -c local ",
    "dev": "ng serve -c localdev ",
    "prebuild": "node generate-build-hash.js",
    "build": "ng build --base-href='/'",
    "test": "ng test --browsers=ChromeNoSandbox --watch=false --source-map=false --code-coverage",
    "testlocal": "ng test --configuration local --code-coverage --browsers=Chrome --reporters progress,kjhtml",
    "testSpec": "ng test --configuration local --code-coverage --watch=false --browsers=ChromeNoSandbox --reporters spec",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "sonar": "sonar-scanner",
    "sonar-scanner": "C:\\sonar-scanner\\bin\\sonar-scanner.bat",
    "packagr": "ng-packagr -p ng-package.json",
    "publish": "npm run packagr && npm publish dist/",
    "clean-cache": "rm -rf node_modules/ package-lock.json && npm cache clean -f && npm i"
  },
  "private": true,
  "dependencies": {
    "@administrativo/components": "^18.0.24",
    "@administrativo/comum": "^11.7.11",
    "@angular/animations": "^18.2.13",
    "@angular/cdk": "^18.2.13",
    "@angular/common": "^18.2.13",
    "@angular/compiler": "^18.2.13",
    "@angular/core": "^18.2.13",
    "@angular/forms": "^18.2.13",
    "@angular/platform-browser": "^18.2.13",
    "@angular/platform-browser-dynamic": "^18.2.13",
    "@angular/router": "^18.2.13",
    "@auth0/angular-jwt": "^5.0.2",
    "@formio/angular": "^7.0.0",
    "@ng-bootstrap/ng-bootstrap": "^14.0.1",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.11.3",
    "core-js": "^3.8.1",
    "flat": "^6.0.1",
    "font-awesome": "^4.7.0",
    "formiojs": "^4.18.0",
    "handlebars": "^4.7.8",
    "html2pdf": "^0.0.11",
    "html2pdf.js": "^0.10.1",
    "jquery": "^3.7.1",
    "jquery-ui": "^1.13.1",
    "jquery-ui-dist": "^1.13.1",
    "jwt-decode": "^3.1.2",
    "karma-sonarqube-reporter": "^1.4.0",
    "lodash": "^4.17.21",
    "material-icons": "^1.13.2",
    "moment": "^2.29.4",
    "ng2-pdfjs-viewer": "^18.0.0",
    "ngx-bootstrap": "^12.0.0",
    "ngx-mask": "^17.1.1",
    "ngx-pagination": "^5.0.0",
    "ngx-select-dropdown": "^3.3.2",
    "popper.js": "^1.16.1",
    "primeng": "^17.18.10",
    "prism-themes": "^1.5.0",
    "prismjs": "^1.22.0",
    "rxjs": "~7.5.0",
    "sonarqube-scanner": "^3.0.1",
    "tslib": "^2.3.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.13",
    "@angular/cli": "^18.2.13",
    "@angular/compiler-cli": "^18.2.13",
